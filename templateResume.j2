% command for bullet points formatting

\newcommand{\sectionTitle}[1]{
    \noalign{\vspace{0.6em}}
    \multicolumn{2}{@{}>{\raggedright\arraybackslash}p{(\linewidth)}@{}}{ \textbf{\large #1} }\\ 
    \noalign{\vspace{0em}}
    \noalign{\hrule height 0.5pt}
}

\newcommand{\contentEntryWorkExp}[5]{% 
    \noalign{\vspace{0.4em}} % Add some vertical space between entries
    \textbf{#1} & \textbf{\small #2} \\ \small
    #3 & \small #4 \\ % Add job label and location
    \multicolumn{2}{@{}>{\raggedright\arraybackslash}p{(\linewidth - 2\tabcolsep) * \real{1.0000} + 2\tabcolsep}@{}}{%
        \begin{minipage}[t]{\linewidth}
            \raggedright
            \begin{itemize}
                #5 % Items go here
            \end{itemize}
        \end{minipage}}\\
}

\newcommand{\contentEntryPersonalProj}[3]{
  \noalign{\vspace{0.4em}} % Add some vertical space between entries
  \multicolumn{2}{@{}>{\raggedright\arraybackslash}p{(\linewidth - 2\tabcolsep) * \real{1.0000} + 2\tabcolsep}@{}}{%
	\textbf{#1 \textbar{}} \emph{#2}} \\ \multicolumn{2}{@{}>{\raggedright\arraybackslash}p{(\linewidth - 2\tabcolsep) * \real{1.0000} + 2\tabcolsep}@{}}{%
	\begin{minipage}[t]{\linewidth}
		\raggedright
		\begin{itemize}
      #3
		\end{itemize}
	\end{minipage}}\\
}

\newcommand{\contentEntryTechnicalSkills}[1]{
  \noalign{\vspace{0.4em}} % Add some vertical space between entries
  \multicolumn{2}{@{}>{\raggedright\arraybackslash}p{(\linewidth - 2\tabcolsep) * \real{1.0000} + 2\tabcolsep}@{}}{%
	\begin{minipage}[t]{\linewidth}
		\raggedright
		\begin{itemize}
      #1
		\end{itemize}
	\end{minipage}}\\
}
